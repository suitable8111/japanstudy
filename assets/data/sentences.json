[
  {"id": "s001", "japanese": "おはようございます。", "reading": "おはようございます。", "korean": "좋은 아침입니다.", "category": "인사", "level": "N5"},
  {"id": "s002", "japanese": "こんにちは。", "reading": "こんにちは。", "korean": "안녕하세요.", "category": "인사", "level": "N5"},
  {"id": "s003", "japanese": "こんばんは。", "reading": "こんばんは。", "korean": "안녕하세요. (저녁)", "category": "인사", "level": "N5"},
  {"id": "s004", "japanese": "お元気ですか。", "reading": "おげんきですか。", "korean": "잘 지내세요?", "category": "인사", "level": "N5"},
  {"id": "s005", "japanese": "はい、元気です。", "reading": "はい、げんきです。", "korean": "네, 잘 지내요.", "category": "인사", "level": "N5"},
  {"id": "s006", "japanese": "ありがとうございます。", "reading": "ありがとうございます。", "korean": "감사합니다.", "category": "인사", "level": "N5"},
  {"id": "s007", "japanese": "すみません。", "reading": "すみません。", "korean": "실례합니다. / 죄송합니다.", "category": "인사", "level": "N5"},
  {"id": "s008", "japanese": "さようなら。", "reading": "さようなら。", "korean": "안녕히 가세요.", "category": "인사", "level": "N5"},
  {"id": "s009", "japanese": "おやすみなさい。", "reading": "おやすみなさい。", "korean": "안녕히 주무세요.", "category": "인사", "level": "N5"},
  {"id": "s010", "japanese": "いただきます。", "reading": "いただきます。", "korean": "잘 먹겠습니다.", "category": "식사", "level": "N5"},
  {"id": "s011", "japanese": "ごちそうさまでした。", "reading": "ごちそうさまでした。", "korean": "잘 먹었습니다.", "category": "식사", "level": "N5"},
  {"id": "s012", "japanese": "これはいくらですか。", "reading": "これはいくらですか。", "korean": "이것은 얼마예요?", "category": "쇼핑", "level": "N5"},
  {"id": "s013", "japanese": "お水をください。", "reading": "おみずをください。", "korean": "물 주세요.", "category": "식사", "level": "N5"},
  {"id": "s014", "japanese": "トイレはどこですか。", "reading": "といれはどこですか。", "korean": "화장실은 어디예요?", "category": "일상", "level": "N5"},
  {"id": "s015", "japanese": "日本語が少し分かります。", "reading": "にほんごがすこしわかります。", "korean": "일본어를 조금 알아요.", "category": "일상", "level": "N5"},
  {"id": "s016", "japanese": "私は韓国人です。", "reading": "わたしはかんこくじんです。", "korean": "저는 한국 사람입니다.", "category": "자기소개", "level": "N5"},
  {"id": "s017", "japanese": "名前は何ですか。", "reading": "なまえはなんですか。", "korean": "이름이 뭐예요?", "category": "자기소개", "level": "N5"},
  {"id": "s018", "japanese": "何歳ですか。", "reading": "なんさいですか。", "korean": "몇 살이에요?", "category": "자기소개", "level": "N5"},
  {"id": "s019", "japanese": "今日はいい天気ですね。", "reading": "きょうはいいてんきですね。", "korean": "오늘은 좋은 날씨네요.", "category": "날씨", "level": "N5"},
  {"id": "s020", "japanese": "明日は雨が降りますか。", "reading": "あしたはあめがふりますか。", "korean": "내일은 비가 올까요?", "category": "날씨", "level": "N5"},
  {"id": "s021", "japanese": "駅はどこですか。", "reading": "えきはどこですか。", "korean": "역은 어디에 있어요?", "category": "교통", "level": "N5"},
  {"id": "s022", "japanese": "電車は何時に来ますか。", "reading": "でんしゃはなんじにきますか。", "korean": "전철은 몇 시에 와요?", "category": "교통", "level": "N5"},
  {"id": "s023", "japanese": "ここから近いですか。", "reading": "ここからちかいですか。", "korean": "여기서 가까워요?", "category": "교통", "level": "N5"},
  {"id": "s024", "japanese": "タクシーを呼んでください。", "reading": "たくしーをよんでください。", "korean": "택시를 불러주세요.", "category": "교통", "level": "N5"},
  {"id": "s025", "japanese": "何時に出発しますか。", "reading": "なんじにしゅっぱつしますか。", "korean": "몇 시에 출발해요?", "category": "교통", "level": "N5"},
  {"id": "s026", "japanese": "このバスは東京駅に行きますか。", "reading": "このばすはとうきょうえきにいきますか。", "korean": "이 버스는 도쿄역에 가나요?", "category": "교통", "level": "N4"},
  {"id": "s027", "japanese": "メニューを見せてください。", "reading": "めにゅーをみせてください。", "korean": "메뉴를 보여주세요.", "category": "식사", "level": "N5"},
  {"id": "s028", "japanese": "おすすめは何ですか。", "reading": "おすすめはなんですか。", "korean": "추천 메뉴는 뭐예요?", "category": "식사", "level": "N5"},
  {"id": "s029", "japanese": "お会計をお願いします。", "reading": "おかいけいをおねがいします。", "korean": "계산 부탁합니다.", "category": "식사", "level": "N5"},
  {"id": "s030", "japanese": "とても美味しいです。", "reading": "とてもおいしいです。", "korean": "매우 맛있어요.", "category": "식사", "level": "N5"},
  {"id": "s031", "japanese": "辛い物は食べられません。", "reading": "からいものはたべられません。", "korean": "매운 것은 못 먹어요.", "category": "식사", "level": "N4"},
  {"id": "s032", "japanese": "予約をしたいのですが。", "reading": "よやくをしたいのですが。", "korean": "예약을 하고 싶은데요.", "category": "식사", "level": "N4"},
  {"id": "s033", "japanese": "写真を撮ってもいいですか。", "reading": "しゃしんをとってもいいですか。", "korean": "사진을 찍어도 될까요?", "category": "관광", "level": "N4"},
  {"id": "s034", "japanese": "もう一度言ってください。", "reading": "もういちどいってください。", "korean": "한 번 더 말해주세요.", "category": "일상", "level": "N5"},
  {"id": "s035", "japanese": "ゆっくり話してください。", "reading": "ゆっくりはなしてください。", "korean": "천천히 말해주세요.", "category": "일상", "level": "N5"},
  {"id": "s036", "japanese": "日本語で何と言いますか。", "reading": "にほんごでなんといいますか。", "korean": "일본어로 뭐라고 해요?", "category": "일상", "level": "N5"},
  {"id": "s037", "japanese": "意味が分かりません。", "reading": "いみがわかりません。", "korean": "의미를 모르겠어요.", "category": "일상", "level": "N5"},
  {"id": "s038", "japanese": "ちょっと待ってください。", "reading": "ちょっとまってください。", "korean": "잠깐만 기다려주세요.", "category": "일상", "level": "N5"},
  {"id": "s039", "japanese": "大丈夫です。", "reading": "だいじょうぶです。", "korean": "괜찮아요.", "category": "일상", "level": "N5"},
  {"id": "s040", "japanese": "助けてください。", "reading": "たすけてください。", "korean": "도와주세요.", "category": "긴급", "level": "N5"},
  {"id": "s041", "japanese": "病院に行きたいです。", "reading": "びょういんにいきたいです。", "korean": "병원에 가고 싶어요.", "category": "긴급", "level": "N5"},
  {"id": "s042", "japanese": "頭が痛いです。", "reading": "あたまがいたいです。", "korean": "머리가 아파요.", "category": "건강", "level": "N5"},
  {"id": "s043", "japanese": "薬をください。", "reading": "くすりをください。", "korean": "약 주세요.", "category": "건강", "level": "N5"},
  {"id": "s044", "japanese": "チェックインをお願いします。", "reading": "ちぇっくいんをおねがいします。", "korean": "체크인 부탁합니다.", "category": "숙박", "level": "N5"},
  {"id": "s045", "japanese": "部屋の鍵をなくしました。", "reading": "へやのかぎをなくしました。", "korean": "방 열쇠를 잃어버렸어요.", "category": "숙박", "level": "N4"},
  {"id": "s046", "japanese": "Wi-Fiのパスワードは何ですか。", "reading": "わいふぁいのぱすわーどはなんですか。", "korean": "와이파이 비밀번호가 뭐예요?", "category": "숙박", "level": "N5"},
  {"id": "s047", "japanese": "荷物を預かってもらえますか。", "reading": "にもつをあずかってもらえますか。", "korean": "짐을 맡아줄 수 있나요?", "category": "숙박", "level": "N4"},
  {"id": "s048", "japanese": "これを試着してもいいですか。", "reading": "これをしちゃくしてもいいですか。", "korean": "이것을 입어봐도 될까요?", "category": "쇼핑", "level": "N4"},
  {"id": "s049", "japanese": "もう少し安くなりませんか。", "reading": "もうすこしやすくなりませんか。", "korean": "좀 더 싸게 안 될까요?", "category": "쇼핑", "level": "N4"},
  {"id": "s050", "japanese": "カードで払えますか。", "reading": "かーどではらえますか。", "korean": "카드로 결제할 수 있나요?", "category": "쇼핑", "level": "N4"},
  {"id": "s051", "japanese": "他の色はありますか。", "reading": "ほかのいろはありますか。", "korean": "다른 색은 있나요?", "category": "쇼핑", "level": "N5"},
  {"id": "s052", "japanese": "大きいサイズはありますか。", "reading": "おおきいさいずはありますか。", "korean": "큰 사이즈 있나요?", "category": "쇼핑", "level": "N5"},
  {"id": "s053", "japanese": "趣味は何ですか。", "reading": "しゅみはなんですか。", "korean": "취미가 뭐예요?", "category": "자기소개", "level": "N5"},
  {"id": "s054", "japanese": "映画を見るのが好きです。", "reading": "えいがをみるのがすきです。", "korean": "영화 보는 것을 좋아해요.", "category": "취미", "level": "N5"},
  {"id": "s055", "japanese": "週末は何をしますか。", "reading": "しゅうまつはなにをしますか。", "korean": "주말에 뭐 해요?", "category": "일상", "level": "N5"},
  {"id": "s056", "japanese": "友達と買い物に行きます。", "reading": "ともだちとかいものにいきます。", "korean": "친구랑 쇼핑하러 가요.", "category": "일상", "level": "N5"},
  {"id": "s057", "japanese": "毎朝六時に起きます。", "reading": "まいあさろくじにおきます。", "korean": "매일 아침 6시에 일어나요.", "category": "일상", "level": "N5"},
  {"id": "s058", "japanese": "仕事は何時に終わりますか。", "reading": "しごとはなんじにおわりますか。", "korean": "일은 몇 시에 끝나요?", "category": "직장", "level": "N5"},
  {"id": "s059", "japanese": "会議は三時からです。", "reading": "かいぎはさんじからです。", "korean": "회의는 3시부터예요.", "category": "직장", "level": "N5"},
  {"id": "s060", "japanese": "昨日はとても忙しかったです。", "reading": "きのうはとてもいそがしかったです。", "korean": "어제는 매우 바빴어요.", "category": "일상", "level": "N5"},
  {"id": "s061", "japanese": "来週日本に行きます。", "reading": "らいしゅうにほんにいきます。", "korean": "다음 주에 일본에 가요.", "category": "여행", "level": "N5"},
  {"id": "s062", "japanese": "飛行機のチケットを買いました。", "reading": "ひこうきのちけっとをかいました。", "korean": "비행기 표를 샀어요.", "category": "여행", "level": "N5"},
  {"id": "s063", "japanese": "パスポートを見せてください。", "reading": "ぱすぽーとをみせてください。", "korean": "여권을 보여주세요.", "category": "여행", "level": "N5"},
  {"id": "s064", "japanese": "観光地はどこがいいですか。", "reading": "かんこうちはどこがいいですか。", "korean": "관광지는 어디가 좋아요?", "category": "관광", "level": "N4"},
  {"id": "s065", "japanese": "この近くにコンビニはありますか。", "reading": "このちかくにこんびにはありますか。", "korean": "이 근처에 편의점 있나요?", "category": "일상", "level": "N5"},
  {"id": "s066", "japanese": "道に迷いました。", "reading": "みちにまよいました。", "korean": "길을 잃었어요.", "category": "긴급", "level": "N4"},
  {"id": "s067", "japanese": "地図を見せてもらえますか。", "reading": "ちずをみせてもらえますか。", "korean": "지도를 보여줄 수 있나요?", "category": "관광", "level": "N4"},
  {"id": "s068", "japanese": "日本の料理は美味しいです。", "reading": "にほんのりょうりはおいしいです。", "korean": "일본 요리는 맛있어요.", "category": "식사", "level": "N5"},
  {"id": "s069", "japanese": "寿司が一番好きです。", "reading": "すしがいちばんすきです。", "korean": "초밥을 가장 좋아해요.", "category": "식사", "level": "N5"},
  {"id": "s070", "japanese": "ラーメンを食べたいです。", "reading": "らーめんをたべたいです。", "korean": "라멘을 먹고 싶어요.", "category": "식사", "level": "N5"},
  {"id": "s071", "japanese": "アレルギーがあります。", "reading": "あれるぎーがあります。", "korean": "알레르기가 있어요.", "category": "건강", "level": "N5"},
  {"id": "s072", "japanese": "お腹がすきました。", "reading": "おなかがすきました。", "korean": "배가 고파요.", "category": "식사", "level": "N5"},
  {"id": "s073", "japanese": "喉が渇きました。", "reading": "のどがかわきました。", "korean": "목이 말라요.", "category": "식사", "level": "N5"},
  {"id": "s074", "japanese": "暑いですね。", "reading": "あついですね。", "korean": "덥네요.", "category": "날씨", "level": "N5"},
  {"id": "s075", "japanese": "寒いですね。", "reading": "さむいですね。", "korean": "춥네요.", "category": "날씨", "level": "N5"},
  {"id": "s076", "japanese": "桜がきれいですね。", "reading": "さくらがきれいですね。", "korean": "벚꽃이 예쁘네요.", "category": "관광", "level": "N5"},
  {"id": "s077", "japanese": "日本語を勉強しています。", "reading": "にほんごをべんきょうしています。", "korean": "일본어를 공부하고 있어요.", "category": "일상", "level": "N5"},
  {"id": "s078", "japanese": "英語は話せますか。", "reading": "えいごははなせますか。", "korean": "영어는 할 수 있나요?", "category": "일상", "level": "N4"},
  {"id": "s079", "japanese": "電話番号を教えてください。", "reading": "でんわばんごうをおしえてください。", "korean": "전화번호를 알려주세요.", "category": "일상", "level": "N5"},
  {"id": "s080", "japanese": "また会いましょう。", "reading": "またあいましょう。", "korean": "다시 만나요.", "category": "인사", "level": "N5"},
  {"id": "s081", "japanese": "お誕生日おめでとうございます。", "reading": "おたんじょうびおめでとうございます。", "korean": "생일 축하합니다.", "category": "축하", "level": "N5"},
  {"id": "s082", "japanese": "新年あけましておめでとうございます。", "reading": "しんねんあけましておめでとうございます。", "korean": "새해 복 많이 받으세요.", "category": "축하", "level": "N4"},
  {"id": "s083", "japanese": "今何時ですか。", "reading": "いまなんじですか。", "korean": "지금 몇 시예요?", "category": "일상", "level": "N5"},
  {"id": "s084", "japanese": "約束の時間に遅れそうです。", "reading": "やくそくのじかんにおくれそうです。", "korean": "약속 시간에 늦을 것 같아요.", "category": "일상", "level": "N4"},
  {"id": "s085", "japanese": "ここに座ってもいいですか。", "reading": "ここにすわってもいいですか。", "korean": "여기 앉아도 될까요?", "category": "일상", "level": "N5"},
  {"id": "s086", "japanese": "窓を開けてもいいですか。", "reading": "まどをあけてもいいですか。", "korean": "창문을 열어도 될까요?", "category": "일상", "level": "N5"},
  {"id": "s087", "japanese": "どこから来ましたか。", "reading": "どこからきましたか。", "korean": "어디서 오셨나요?", "category": "자기소개", "level": "N5"},
  {"id": "s088", "japanese": "日本に来たのは初めてです。", "reading": "にほんにきたのははじめてです。", "korean": "일본에 온 건 처음이에요.", "category": "여행", "level": "N4"},
  {"id": "s089", "japanese": "三日間滞在します。", "reading": "みっかかんたいざいします。", "korean": "3일간 체류합니다.", "category": "여행", "level": "N4"},
  {"id": "s090", "japanese": "お土産を買いたいです。", "reading": "おみやげをかいたいです。", "korean": "선물을 사고 싶어요.", "category": "쇼핑", "level": "N5"},
  {"id": "s091", "japanese": "これを二つください。", "reading": "これをふたつください。", "korean": "이것 두 개 주세요.", "category": "쇼핑", "level": "N5"},
  {"id": "s092", "japanese": "袋をお願いします。", "reading": "ふくろをおねがいします。", "korean": "봉투 부탁합니다.", "category": "쇼핑", "level": "N5"},
  {"id": "s093", "japanese": "免税はできますか。", "reading": "めんぜいはできますか。", "korean": "면세 되나요?", "category": "쇼핑", "level": "N4"},
  {"id": "s094", "japanese": "返品できますか。", "reading": "へんぴんできますか。", "korean": "반품 가능한가요?", "category": "쇼핑", "level": "N4"},
  {"id": "s095", "japanese": "右に曲がってください。", "reading": "みぎにまがってください。", "korean": "오른쪽으로 돌아주세요.", "category": "교통", "level": "N5"},
  {"id": "s096", "japanese": "まっすぐ行ってください。", "reading": "まっすぐいってください。", "korean": "쭉 직진해주세요.", "category": "교통", "level": "N5"},
  {"id": "s097", "japanese": "次の信号で止めてください。", "reading": "つぎのしんごうでとめてください。", "korean": "다음 신호에서 세워주세요.", "category": "교통", "level": "N4"},
  {"id": "s098", "japanese": "楽しかったです。", "reading": "たのしかったです。", "korean": "즐거웠어요.", "category": "일상", "level": "N5"},
  {"id": "s099", "japanese": "また日本に来たいです。", "reading": "またにほんにきたいです。", "korean": "다시 일본에 오고 싶어요.", "category": "여행", "level": "N5"},
  {"id": "s100", "japanese": "お世話になりました。", "reading": "おせわになりました。", "korean": "신세를 졌습니다.", "category": "인사", "level": "N4"}
]
